/*快递信息*/
.kd{
	width: 100%;
	height: auto;
	position: relative;
	border-top: 7px solid #ddd;
	padding-top: 20rpx;
}
.inde-one{
	padding: 0 30rpx;
	height: auto;
	line-height: 50rpx;
	font-size: 28rpx;
	margin-top: 5rpx;
	color: #666;
}
.inde-kd{
	padding: 0 30rpx;
	height: 100rpx;
	display: flex;
	line-height: 40rpx;
}
.inde-kd-left{
	width: 150rpx;
	height: 100rpx;
	line-height: 40rpx;
	font-size: 24rpx;
	color: #666;
}
.inde-kd-center{
	width: 20rpx;
	height: 100rpx;
	border-left: 2px solid #999;
	margin-left: 15rpx;
	position: relative;
}
.index-kd-ceyuan{
	width: 20rpx;
	height: 20rpx;
	border-radius: 50%;
	background: #999;
	position: absolute;
	top: 32rpx;
	left: -10rpx;
}
.inde-kd-right{
	flex: 1;
	font-size: 28rpx;
	padding-left: 10rpx;
	color: #333;
	height: 80rpx;
	line-height: 40rpx;
	overflow: hidden;
}
.inde-kd-right1{
	flex: 1;
	font-size: 28rpx;
	padding-left: 10rpx;
	color: #333;
	height: 80rpx;
	line-height: 80rpx;
	overflow: hidden;
}
.zuihou{
	height: 40rpx;
}
.zuihou image{
	position: absolute;
	top: 30rpx;
	left: -18rpx;
}
.chushi{
	height: 80rpx;
	margin-top: 30rpx;
}
.chushi image{
	position: absolute;
	top: 0;
	left: -18rpx;
}
.kd button{
	width: 250rpx;
	height: 65rpx;
	background: #BC924E;
	color: #fff;
	text-align: center;
	line-height: 65rpx;
	border-radius: 20rpx;
	font-size: 28rpx;
	position: absolute;
	top: 90rpx;
	right: 50rpx;
}
.kd-zw{
	font-size: 28rpx;
	color: #fff;
	text-align: center;
	margin: 30rpx;
}